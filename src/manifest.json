{
  "name": "Cambridge Dictionary Copier",
  "description": "Cambridge Dictionary Copier",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": [
        "scripts/index.js"
      ],
      "matches": [
        "https://dictionary.cambridge.org/dictionary/*"
      ]
    }
  ],
  "action": {
    "default_title": "Cambridge Dictionary Copier",
    "default_icon": "assets/icon.png"
  }
}
