{
  "name": "Cambridge Dictionary Copier",
  "description": "An extension that allows you to copy the word information with one click.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": ["tabs","favicon"],
  "icons": {
    "16": "assets/logo-16.png",
    "32": "assets/logo-32.png",
    "48": "assets/logo-48.png",
    "128": "assets/logo-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/index.js"
      ],
      "matches": [
        "https://dictionary.cambridge.org/dictionary/*"
      ]
    }
  ],
  "action": {
    "default_title": "Cambridge Dictionary Copier",
    "default_icon": {
      "16": "assets/logo-16.png",
      "32": "assets/logo-32.png",
      "48": "assets/logo-48.png",
      "128": "assets/logo-128.png"
    }
  }
}
